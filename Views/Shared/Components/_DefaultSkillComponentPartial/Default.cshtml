@model List<ResumeProjectDemoNight.Entities.Skill>
<div class="col-md-6">
	<!-- skill item -->
	@foreach(var item in Model){
	<div class="skill-item">
		<div class="skill-info clearfix">
			<h4 class="float-left mb-3 mt-0">@item.Name</h4>
				<span class="float-right">@item.SkillValue%</span>
		</div>
		<div class="progress">
			<div class="progress-bar data-background" role="progressbar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="@item.SkillValue" data-color="#FFD15C">
			</div>
		</div>
		<div class="spacer" data-height="20"></div>
		</div>
	}
</div>